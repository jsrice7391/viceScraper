
<h1 class="center-align">Saved Articles</h1>

<div class="container">

{{#each articles}}
    <div class="row">
        <div class="col s12 m6">
            <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                   <a href="/article/{{title}}"><span class="card-title">{{title}}</span></a>
                    <p>{{subText}}</p>
                </div>
                <div class="card-action">
                </div>
            </div>
        </div>
    </div>
{{/each}}

</div>